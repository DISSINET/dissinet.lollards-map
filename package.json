{
  "name": "dissinet.cisterciennes",
  "version": "0.0.1",
  "description": "scrapping and analysing cisterciennes wiki table",
  "main": "index.js",
  "repository": "git@github.com:adammertel/dissinet.cisterciennes.git",
  "author": "adammertel <mertel.adam@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "csvtojson": "^2.0.8",
    "request": "^2.88.0",
    "ya-csv": "^0.10.1"
  },
  "scripts": {
    "scrap": "nodemon scrap.js",
    "clean-build": "rm -f -r -d build",
    "start": "parcel app/index.html --open --out-dir build",
    "build": "yarn run clean-build && parcel build app/index.html --out-dir build --no-cache --public-url ./",
    "deploy": "rsync -avz build/ hde@hde.geogr.muni.cz:/var/www/virthtml/hde/cisterciansfrance --delete",
    "build-deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.7.2",
    "chroma-js": "^1.4.0",
    "d3": "^5.7.0",
    "leaflet": "^1.3.4",
    "leaflet.markercluster": "^1.4.1",
    "leaflet.markercluster.placementstrategies": "^0.1.6",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.15.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6"
  }
}
